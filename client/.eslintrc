{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "react/require-default-props": 0,
    "no-alert": 0,
    "no-underscore-dangle": 0,
    "no-console": ["error", { "allow": ["log"] }],
    "react/prop-types": 0,
    "jsx-a11y/href-no-hash": 0
  },
  "globals": {
    "document": true,
    "alert": true,
    "window": true
  }
}